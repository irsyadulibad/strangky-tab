{
    "manifest_version": 3,
    "name": "Strangky",
    "description": "Strangky is extension for tab custom, you can customize the tab chrome natively, Created By Mohammad Sahrullah",
    "version": "0.1",
    "chrome_url_overrides": {
        "newtab": "dist/index.html"
    },
    "background": {
        "service_worker": "dist/background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "management",
        "bookmarks",
        "sessions",
        "unlimitedStorage",
        "tabs"],
    "action":{
        "default_popup": "dist/popup.html"
    }
}
